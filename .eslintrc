{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-constant-condition": [1,{ "checkLoops": false }],
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-boolean-value": 0,
    "react/prop-types": 0,
    "camelcase": 0,
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
  }
}
